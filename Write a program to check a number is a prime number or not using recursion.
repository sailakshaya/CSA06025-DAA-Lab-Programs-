def is_prime(n, divisor=None):
    if n <= 1:
        return False
    if divisor is None:
        return is_prime(n, 2)
    if divisor * divisor > n:
        return True
    if n % divisor == 0:
        return False
    return is_prime(n, divisor + 1)

# Example usage
num = 29
print(is_prime(num))  # Output: True
