def fibonacci(n):
    if n <= 1:
        return n
    else:
        return fibonacci(n - 1) + fibonacci(n - 2)

def print_fibonacci_series(terms):
    for i in range(terms):
        print(fibonacci(i), end=' ')
    print()  # For newline at the end

# Example usage
num_terms = 10
print_fibonacci_series(num_terms)
